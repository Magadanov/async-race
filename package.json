{
    "name": "async-race",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "format": "prettier --write src/**/*.{tsx,jsx,json,css,scss,html}",
        "ci:format": "prettier --check \"**/*.*\"",
        "start": "webpack serve --config ./webpack.config.js --env mode=dev",
        "build": "webpack --config ./webpack.config.js --env mode=prod",
        "lint": "eslint src --ext .ts"
    },
    "dependencies": {
        "@babel/core": "^7.24.4",
        "@babel/plugin-transform-runtime": "^7.24.3",
        "@babel/preset-env": "^7.24.4",
        "@babel/preset-react": "^7.24.1",
        "@babel/preset-typescript": "^7.24.1",
        "@types/react": "^18.2.79",
        "@types/react-dom": "^18.2.25",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^7.1.1",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-hooks": "^0.4.3",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^28.2.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-webpack-plugin": "^4.1.0",
        "html-webpack-plugin": "^5.6.0",
        "prettier": "^3.2.5",
        "prettier-eslint": "^16.3.0",
        "prettier-eslint-cli": "^8.0.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.1.0",
        "react-router-dom": "^6.22.3",
        "sass": "^1.75.0",
        "sass-loader": "^14.2.1",
        "style-loader": "^4.0.0",
        "typescript": "^5.4.5",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-merge": "^5.10.0"
    },
    "devDependencies": {
        "husky": "^4.0.0",
        "lint-staged": "^15.2.2",
        "validate-branch-name": "^1.3.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "validate-branch-name"
        }
    },
    "lint-staged": {
        "src/**/*.{tsx,jsx,json,css,scss,md,html}": "npm run format"
    },
    "validate-branch-name": {
        "pattern": "^((main|master|async-race)|(feat|refactor|init|docs)/AR-[0-9]{2}_[a-zA-Z0-9_-]+)$",
        "errorMsg": "write correct branch name"
    }
}
